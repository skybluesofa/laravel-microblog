{
    "name": "skybluesofa/laravel-microblog",
    "description": "This package creates the ability to create micro blog posts for Eloquent Users.",
    "keywords": ["laravel", "status", "posts", "microblog", "blog", "eloqent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Rogers",
            "email": "connects@skybluesofa.com"
        }
    ],
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "require": {
        "php": ">=7.1.0",
        "webpatser/laravel-uuid": "2.*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "fzaninotto/faker": "~1.4",
        "codeclimate/php-test-reporter": "^0.3.2"
    },
    "autoload": {
        "psr-4": {
            "Skybluesofa\\Microblog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Skybluesofa\\Microblog\\ServiceProvider"
            ]
        }
    }
}
