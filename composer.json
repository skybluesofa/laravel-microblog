{
    "name": "skybluesofa/laravel-microblog",
    "description": "This package creates the ability to create micro blog posts for Eloquent Users.",
    "keywords": [
        "laravel",
        "status",
        "posts",
        "microblog",
        "blog",
        "eloqent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Rogers",
            "email": "connects@skybluesofa.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "webpatser/laravel-uuid": "2.*",
        "laravel/legacy-factories": "^1.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "orchestra/testbench": "^7.11",
        "laravel/pint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Skybluesofa\\Microblog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skybluesofa\\Microblog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Skybluesofa\\Microblog\\ServiceProvider"
            ]
        }
    }
}
